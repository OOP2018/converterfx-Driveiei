<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="converter.ConverterController">
   <top>
      <FlowPane hgap="8.0" prefHeight="114.0" prefWidth="796.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="textfield1" onAction="#handleConvert" prefHeight="32.0" prefWidth="155.0" />
            <ComboBox fx:id="combobox1" prefHeight="31.0" prefWidth="136.0" />
            <TextField fx:id="textfield2" onAction="#handleConvert" />
            <ComboBox fx:id="combobox2" prefHeight="31.0" prefWidth="126.0" />
            <Button mnemonicParsing="false" onAction="#handleConvert" text="Convert" />
            <Button mnemonicParsing="false" onAction="#handleClear" text="Clear" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </FlowPane>
   </top>
</BorderPane>
